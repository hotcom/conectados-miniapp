{
  "name": "conectados-miniapp",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "env": {
    "NEXT_PUBLIC_MINIAPP_MODE": "true",
    "NEXT_PUBLIC_CHAIN_ID": "84532",
    "NEXT_PUBLIC_NETWORK_NAME": "Base Sepolia"
  }
}
